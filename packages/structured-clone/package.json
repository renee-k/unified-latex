{
  "name": "@unified-latex/structured-clone",
  "version": "1.4.2",
  "description": "A cheap (and not correct) structured-clone polyfill with types",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.map",
    "dist/**/*.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*js": "./dist/*js",
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "build": "npm run clean && mkdirp ./dist && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "wireit",
    "compile:types": "tsc -b tsconfig.json",
    "compile:js": "node build.js",
    "test": "vitest"
  },
  "wireit": {
    "compile": {
      "dependencies": ["compile:types", "compile:js"],
      "files": ["src/**/*.ts", "index.ts", "tsconfig.json"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siefkenj/unified-latex.git"
  },
  "keywords": [
    "pegjs",
    "latex",
    "parser",
    "prettier",
    "unified-latex",
    "unified"
  ],
  "author": "Jason Siefken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siefkenj/unified-latex/issues"
  },
  "homepage": "https://github.com/siefkenj/unified-latex#readme"
}
